@model personapi_dotnet.Models.Telefono

@{
    ViewData["Title"] = "Detalle de Teléfono";
}

<h1>@ViewData["Title"]</h1>

<dl class="row">
    <dt class="col-sm-2">Número</dt>
    <dd class="col-sm-10">@Model.Num</dd>
    <dt class="col-sm-2">Operadora</dt>
    <dd class="col-sm-10">@Model.Oper</dd>
    <dt class="col-sm-2">Dueño CC</dt>
    <dd class="col-sm-10">@Model.Duenio</dd>
</dl>

<p>
    <a class="btn btn-secondary" asp-action="Index">Volver</a>
    <a class="btn btn-warning"
       asp-action="Edit"
       asp-route-id="@Model.Num">Editar</a>
</p>
